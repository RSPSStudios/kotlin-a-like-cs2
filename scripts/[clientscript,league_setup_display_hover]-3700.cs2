fun league_setup_display_hover(boolean0: Boolean, component1: Component, colour2: Int) {

	if (boolean0 = true) {
		if (cc_find(component1, 0) = ^true) {
			cc_settrans(100)
		}
		if (cc_find(component1, 3) = ^true) {
			cc_settrans(125)
			cc_setgraphic("scrollbar_v2,1")
		}
		if (cc_find(component1, 4) = ^true) {
			cc_setcolour(colour2)
		}
	} else {
		~league_setup_display_hoveroff(component1, colour2)
	}
	return 
}
