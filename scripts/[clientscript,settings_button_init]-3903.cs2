// 3903 clientscript
fun settings_button_init(component0: Component, component1: Component) {

	val comsubid2: Int = ~v2_stone_button_filled(component0)
	cc_create(component0, ^iftype_text, comsubid2)
	cc_setsize(10, 10, ^setsize_minus, ^setsize_minus)
	cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_centre)
	cc_settext("All Settings")
	cc_settextfont(p11_full)
	cc_settextshadow(true)
	cc_setcolour(0xff981f)
	cc_settextalign(^settextalign_centre, ^settextalign_centre, 14)
	if (~on_mobile = false) {
		if_setonmouserepeat {
			settings_button_repeat(component0, component1)
		}
		if_setonmouseleave {
			settings_button_leave(component0, component1)
		}
	}
	if_setonop {
		settings_side_op_sound
	}
	return 
}
