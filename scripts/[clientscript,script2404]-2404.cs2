// 2404 clientscript
fun script2404(component0: Component, comsubid1: Int, clock2: Int) {

	val int3: Int = 0
	if (cc_find(component0, comsubid1) = ^true) {
		int3 = calc(clientclock - clock2)
		if (int3 < 40) {
			cc_settrans(scale(int3, 40, 255))
			return 
		}
		cc_deleteall(component0)
	}
	return 
}
