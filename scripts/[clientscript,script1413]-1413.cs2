// 1413 clientscript
fun script1413(component0: Component, model1: Model, component2: Component, clock3: Int) {

	if (clientclock < clock3) {
		return 
	}
	if_setmodel(model1, component0)
	if_sethide(true, component2)
	if_setonmouserepeat({
		comp_sethide(false, component2)
	}, component0)
	if_setonmouseleave({
		comp_sethide(true, component2)
	}, component0)
	if_setontimer(null, component0)
	return 
}
