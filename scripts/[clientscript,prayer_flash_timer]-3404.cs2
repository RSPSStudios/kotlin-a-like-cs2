// 3404 clientscript
fun prayer_flash_timer(component0: Component, int1: Int, int2: Int) {

	int1 = calc(int1 - 1)
	if (int1 <= 0 & cc_find(component0, 1) = ^true) {
		int1 = 15
		if (int2 = 1) {
			cc_sethide(true)
		} else {
			cc_sethide(false)
		}
		int2 = calc(1 - int2)
	}
	if_setontimer({
		prayer_flash_timer(component0, int1, int2)
	}, component0)
	return 
}
