fun death_coffer_activebutton(component0: Component, string0: String, component1: Component) {

	~death_coffer_drawbutton(true, false, component0, string0, component1)
	if_setonmouserepeat({
		death_coffer_drawbutton(true, true, component0, string0, component1)
	}, component0)
	if_setonmouseleave({
		death_coffer_drawbutton(true, false, component0, string0, component1)
	}, component0)
	if_setonop({
		death_coffer_opbutton(event_opindex, component0, string0, component1)
	}, component0)
	if_setontimer(null, component0)
	return 
}
