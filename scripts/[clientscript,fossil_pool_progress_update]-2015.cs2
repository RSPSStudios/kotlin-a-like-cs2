	if_setsize(interpolate(0, 16384, 0, 10, int0), 0, ^setsize_2, ^setsize_minus, interface_606:18)
	val int1: Int = 0
	val int2: Int = 0
	if (int0 <= 2) {
		int1, int2 = 255, interpolate(0, int0, 0, 2, 255)
	} else {
		int1, int2 = interpolate(0, int0, 0, 10, 255), 255
	}
	if_setcolour(~rgb_to_hex(int1, int2, 0), interface_606:18)
	return 
}
