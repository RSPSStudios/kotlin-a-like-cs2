// 799 clientscript
fun ge_pricelist(inv0: Inv, string0: String) {

	cc_deleteall(interface_237:1)
	~steelborder(interface_237:1, "Grand Exchange: ${string0}", 0)
	cc_deleteall(interface_237:3)
	~scrollbar_vertical(interface_237:3, interface_237:2, "scrollbar_dragger_v2,3", "scrollbar_dragger_v2,0", "scrollbar_dragger_v2,1", "scrollbar_dragger_v2,2", "scrollbar_v2,0", "scrollbar_v2,1")
	cc_deleteall(interface_237:4)
	cc_create(interface_237:4, ^iftype_graphic, 0)
	cc_setsize(17, 17, ^setsize_abs, ^setsize_abs)
	cc_setposition(0, 0, ^setpos_abs_left, ^setpos_abs_top)
	val string1: String = "Column view"
	val width1: Int = calc(parawidth(string1, 512, p12_full) + 20)
	if_setsize(width1, 17, ^setsize_abs, ^setsize_abs, interface_237:4)
	.cc_create(interface_237:4, ^iftype_text, 1)
	.cc_setsize(width1, 17, ^setsize_abs, ^setsize_abs)
	.cc_setposition(0, 0, ^setpos_abs_left, ^setpos_abs_top)
	.cc_setcolour(0xff981f)
	if_setonmouseover({
		cc_colour_swapper(event_com, .cc_getid, ^white)
	}, interface_237:4)
	if_setonmouseleave({
		cc_colour_swapper(event_com, .cc_getid, 0xff981f)
	}, interface_237:4)
	.cc_settextfont(p12_full)
	.cc_settextshadow(true)
	.cc_settextalign(^settextalign_right, ^settextalign_centre, 0)
	.cc_settext(string1)
	val size2: Int = inv_size(inv0)
	if_setoninvtransmit({
		ge_pricelist_draw(inv0, size2, cc_getid){inv0}
	}, interface_237:0)
	if_setonvartransmit({
		ge_pricelist_draw(inv0, size2, cc_getid){VARP[499]}
	}, interface_237:0)
	if_setonop({
		ge_pricelist_switchmode(event_opindex, inv0, size2, cc_getid)
	}, interface_237:4)
	~ge_pricelist_draw(inv0, size2, cc_getid)
	return 
}
