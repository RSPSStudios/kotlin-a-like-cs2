// 687 clientscript
fun ironman_init(component0: Component, component1: Component, component2: Component, component3: Component, component4: Component, component5: Component, component6: Component, component7: Component, component8: Component, component9: Component, component10: Component, component11: Component, component12: Component, component13: Component, component14: Component) {

	~steelborder(component2, "${text_gender("Iron Man", "Iron Woman")} Setup", 0)
