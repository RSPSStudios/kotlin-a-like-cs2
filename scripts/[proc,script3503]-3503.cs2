// 3503 proc
fun script3503(component0: Component, boolean1: Boolean, boolean2: Boolean) {

	val comsubid3: Int = 0
	if (boolean1 = true | boolean2 = true) {
		comsubid3 = ~v2_stone_button_in_filled(component0)
	} else {
		comsubid3 = ~v2_stone_button_filled(component0)
	}
	cc_create(component0, ^iftype_graphic, comsubid3)
	cc_setsize(29, 22, ^setsize_abs, ^setsize_abs)
	cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_centre)
	cc_setgraphic("bankbuttons,4")
	if (boolean2 = true) {
		cc_settrans(0)
	} else {
		cc_settrans(50)
	}
	return 
}
