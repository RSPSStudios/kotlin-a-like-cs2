// 2825 clientscript
fun seed_vault_drag_text(component0: Component, component1: Component, comsubid2: Int, mousex3: Int, mousey4: Int, component5: Component, component6: Component, clock7: Int) {

	if (clock7 ! VARCINT[374]) {
		return 
	}
	if (cc_find(component0, comsubid2) = ^true & .cc_find(component1, comsubid2) = ^true) {
		cc_setposition(calc(mousex3 + 7), calc(mousey4 - 32), ^setpos_abs_left, ^setpos_abs_top)
	}
	~seed_vault_doscroll(component5, component6, mousey4)
	return 
}
