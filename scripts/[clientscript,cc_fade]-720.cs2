// 720 clientscript
fun cc_fade(component0: Component, int1: Int, int2: Int, trans3: Int, int4: Int) {

	if (int4 = 0) {
		return 
	}
	if (trans3 < 0) {
		trans3 = 0
	} else if (trans3 > 255) {
		trans3 = 255
	}
	val int5: Int = int1
	while (int5 <= int2) {
		if (cc_find(component0, int5) = ^true) {
			cc_setontimer({
				cc_fade_timer(event_com, event_comsubid, trans3, int4)
			})
		}
		int5 = calc(int5 + 1)
	}
	return 
}
