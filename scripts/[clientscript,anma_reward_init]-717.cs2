fun anma_reward_init(component0: Component, component1: Component) {

	~v2_stone_button_out_filled(component1)
	if_setonmouseover {
		v2_stone_button_in_filled(component1)
	}
	if_setonmouseleave {
		v2_stone_button_out_filled(component1)
	}
	if_setonhold {
		v2_stone_button_in_filled(component1)
	}
	if_setonrelease {
		v2_stone_button_out_filled(component1)
	}
	if_setonop {
		opsound(event_opindex, 14)
	}
	return 
}
