// 2098 proc
fun autocast_setup(component0: Component, component1: Component) {

	cc_deleteall(component0)
	cc_create(component0, ^iftype_text, 0)
	cc_setsize(parawidth("Cancel", 190, p12_full), 15, ^setsize_abs, ^setsize_abs)
	cc_setposition(5, 0, ^setpos_abs_right, ^setpos_abs_bottom)
	cc_settextalign(^settextalign_right, ^settextalign_centre, 0)
	cc_setcolour(0x8f8f8f)
	cc_setonmouseover({
		cc_colour_swapper(event_com, event_comsubid, 0xcfcfcf)
	})
	cc_setonmouseleave({
		cc_colour_swapper(event_com, event_comsubid, 0x8f8f8f)
	})
	cc_settextfont(p12_full)
	cc_settextshadow(true)
	cc_settext("Cancel")
	cc_setop(1, "Cancel")
	val int2: Int = 1
	val x3: Int = -1
	val y4: Int = -1
	val obj5: Obj = null
	while (int2 <= 52) {
		cc_create(component0, ^iftype_graphic, int2)
		x3, y4, obj5 = ~autocast_spellpos(int2, VARP[664]), enum(int, obj, enum_1986, int2)
		if (x3 ! -1 & y4 ! -1 & obj5 ! null) {
			cc_setposition(x3, y4, ^setpos_abs_centre, ^setpos_abs_centre)
			cc_setsize(24, 24, ^setsize_abs, ^setsize_abs)
			cc_setop(1, oc_param(obj5, spell_name))
			cc_setoninvtransmit({
				autocast_spellhighlight(event_com, int2, obj5){inv, worn}
			})
			cc_setonstattransmit({
				autocast_spellhighlight(event_com, int2, obj5){magic}
			})
			cc_setonvartransmit({
				autocast_spellhighlight(event_com, int2, obj5){VARP[638], VARP[375], VARP[1139], VARP[1140], VARP[1377], VARP[1105]}
			})
			~autocast_spellhighlight(obj5)
			cc_setonmouseover({
				autocast_tooltip(component1, obj5)
			})
			cc_setonmouseleave({
				autocast_tooltip(component1, null)
			})
		} else {
			cc_sethide(true)
		}
		int2 = calc(int2 + 1)
	}
	cc_deleteall(component1)
	cc_create(component1, ^iftype_text, 0)
	cc_setsize(4, 4, ^setsize_minus, ^setsize_minus)
	cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_centre)
	cc_setcolour(0xafaf9f)
	cc_settextalign(^settextalign_centre, ^settextalign_centre, 0)
	cc_settextfont(p12_full)
	cc_settextshadow(true)
	cc_settext("Select a Combat Spell")
	cc_create(component1, ^iftype_text, 1)
	cc_setsize(4, 18, ^setsize_minus, ^setsize_abs)
	cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_top)
	cc_setcolour(0xefef00)
	cc_settextalign(^settextalign_centre, ^settextalign_bottom, 0)
	cc_settextfont(p12_full)
	cc_settextshadow(true)
	cc_settext("")
	cc_create(component1, ^iftype_text, 2)
	cc_setsize(4, 20, ^setsize_minus, ^setsize_abs)
	cc_setposition(0, 18, ^setpos_abs_centre, ^setpos_abs_top)
	cc_setcolour(0x9f9f00)
	cc_settextalign(^settextalign_centre, ^settextalign_centre, 0)
	cc_settextfont(p11_full)
	cc_settextshadow(true)
	cc_settext("")
	cc_create(component1, ^iftype_model, 3)
	cc_setsize(32, 32, ^setsize_abs, ^setsize_abs)
	cc_create(component1, ^iftype_text, 4)
	cc_setsize(32, 68, ^setsize_abs, ^setsize_minus)
	cc_settextfont(p11_full)
	cc_settextalign(^settextalign_centre, ^settextalign_top, 0)
	cc_settextshadow(false)
	cc_create(component1, ^iftype_model, 5)
	cc_setsize(32, 32, ^setsize_abs, ^setsize_abs)
	cc_create(component1, ^iftype_text, 6)
	cc_setsize(32, 68, ^setsize_abs, ^setsize_minus)
	cc_settextfont(p11_full)
	cc_settextalign(^settextalign_centre, ^settextalign_top, 0)
	cc_settextshadow(false)
	cc_create(component1, ^iftype_model, 7)
	cc_setsize(32, 32, ^setsize_abs, ^setsize_abs)
	cc_create(component1, ^iftype_text, 8)
	cc_setsize(32, 68, ^setsize_abs, ^setsize_minus)
	cc_settextfont(p11_full)
	cc_settextalign(^settextalign_centre, ^settextalign_top, 0)
	cc_settextshadow(false)
	cc_create(component1, ^iftype_model, 9)
	cc_setsize(32, 32, ^setsize_abs, ^setsize_abs)
	cc_create(component1, ^iftype_text, 10)
	cc_setsize(32, 68, ^setsize_abs, ^setsize_minus)
	cc_settextfont(p11_full)
	cc_settextalign(^settextalign_centre, ^settextalign_top, 0)
	cc_settextshadow(false)
	~autocast_tooltip(component1, null)
	return 
}
