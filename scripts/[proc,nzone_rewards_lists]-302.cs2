// 302 proc
fun nzone_rewards_lists(component0: Component, component1: Component) {

	cc_deleteall(component0)
	cc_deleteall(component1)
	val width2: Int = if_getwidth(component0)
	val int3: Int = calc(width2 / 7)
	val int4: Int = calc((int3 - 36) / 2)
	val int5: Int = calc(32 + 20)
	val int6: Int = 0
	val namedobj7: Namedobj = enum(int, namedobj, enum_709, int6)
	while (namedobj7 ! null) {
		cc_create(component0, ^iftype_graphic, calc(int6 * 2))
		.cc_create(component0, ^iftype_text, calc(int6 * 2 + 1))
		cc_setposition(calc(int4 + int6 % 7 * int3), calc(int6 / 7 * int5), ^setpos_abs_left, ^setpos_abs_top)
		.cc_setposition(cc_getx, calc(cc_gety + 32), ^setpos_abs_left, ^setpos_abs_top)
		~nzone_rewards_lists_drawitem(namedobj7)
		int6 = calc(int6 + 1)
		namedobj7 = enum(int, namedobj, enum_709, int6)
	}
	val string0: String = "These items will give you a benefit in future Nightmare Zone dreams, and cannot be used elsewhere.<br><br>Dominic's potions are sold by the dose. When you purchase a dose, it is automatically stored in Dominic's barrels until you collect it.<br><br>The figures in <col=ffffff>white</col> show the number of doses you've got stored already."
	width2 = if_getwidth(component1)
	val int8: Int = calc(paraheight(string0, width2, p12_full) * 12 + 5)
	int3 = calc(width2 / 7)
	int4 = calc((int3 - 36) / 2)
	int5 = calc(32 + 40)
	int6 = 0
	namedobj7 = enum(int, namedobj, enum_814, int6)
	while (namedobj7 ! null) {
		cc_create(component1, ^iftype_graphic, calc(int6 * 3))
		.cc_create(component1, ^iftype_text, calc(int6 * 3 + 1))
		cc_setposition(calc(int4 + int6 % 7 * int3), calc(int8 + int6 / 7 * int5), ^setpos_abs_left, ^setpos_abs_top)
		.cc_setposition(cc_getx, calc(cc_gety + 32), ^setpos_abs_left, ^setpos_abs_top)
		~nzone_rewards_lists_drawitem(namedobj7)
		cc_create(component1, ^iftype_text, calc(int6 * 3 + 2))
		cc_setsize(36, 20, ^setsize_abs, ^setsize_abs)
		cc_setposition(.cc_getx, calc(.cc_gety + .cc_getheight), ^setpos_abs_left, ^setpos_abs_top)
		cc_settextfont(p12_full)
		cc_setcolour(^white)
		cc_settextshadow(true)
		cc_settextalign(^settextalign_centre, ^settextalign_top, 0)
		~nzone_rewards_updatepotion(namedobj7)
		cc_setonvartransmit {
			nzone_rewards_updatepotion(event_com, event_comsubid, namedobj7){VARP[1066]}
		}
		int6 = calc(int6 + 1)
		namedobj7 = enum(int, namedobj, enum_814, int6)
	}
	cc_create(component1, ^iftype_text, calc(int6 * 3))
	cc_setsize(width2, int8, ^setsize_abs, ^setsize_abs)
	cc_setposition(0, 0, ^setpos_abs_left, ^setpos_abs_top)
	cc_settextfont(p12_full)
	cc_setcolour(0xff981f)
	cc_settextalign(^settextalign_left, ^settextalign_top, 0)
	cc_settextshadow(true)
	cc_settext(string0)
	return 
}
