// 1544 clientscript
fun raids_sidepanel_resetbutton(clock0: Int, string0: String, component1: Component) {

	if (clientclock < clock0) {
		return 
	}
	~raids_sidepanel_drawbutton(false, string0, component1)
	if_setonmouserepeat({
		raids_sidepanel_drawbutton(true, string0, component1)
	}, component1)
	if_setonmouseleave({
		raids_sidepanel_drawbutton(false, string0, component1)
	}, component1)
	if_setonop({
		raids_sidepanel_opbutton(event_opindex, string0, component1)
	}, component1)
	if_setontimer(null, component1)
	return 
}
