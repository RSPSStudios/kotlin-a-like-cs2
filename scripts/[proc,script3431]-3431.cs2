// 3431 proc
fun script3431(component0: Component) {

	val int1: Int = 0
	val x2: Int = 0
	val y3: Int = 0
	val int4: Int = 0
	val graphic5: Graphic = null
	while (int1 < 25) {
		cc_create(component0, ^iftype_graphic, int1)
		cc_setsize(35, 35, ^setsize_abs, ^setsize_abs)
		x2 = calc(35 * (int1 % 5))
		y3 = calc(35 * (int1 / 5))
		cc_setposition(x2, y3, ^setpos_abs_left, ^setpos_abs_top)
		cc_setgraphic("graphic_2540")
		cc_settrans(0)
		cc_setop(1, "Toggle")
		cc_setonop({
			script3432(int1, event_com)
		})
		cc_setonvartransmit({
			script3434(int1, event_com){VARP[2715], VARP[2716]}
		})
		if (~on_mobile = false) {
			cc_setonmouserepeat({
				script3433(int1, event_com, true)
			})
			cc_setonmouseleave({
				script3433(int1, event_com, false)
			})
		}
		int4 = ~script3439(int1)
		if (int4 = 0) {
			graphic5 = "graphic_2540"
		} else if (int4 = 1) {
			graphic5 = "graphic_2541"
		} else if (int4 = 2) {
			graphic5 = "graphic_2542"
		}
		cc_setgraphic(graphic5)
		int1 = calc(int1 + 1)
	}
	~script3435
	return 
}
