// 347 clientscript
fun pattern_cards_confirm_reset(clock0: Int, component1: Component) {

	if (clientclock < clock0) {
		return 
	}
	if_setontimer(null, component1)
	~clan_stonebutton_init("Confirm", component1, "p12_full")
	if_setonop({
		pattern_cards_confirm_op(event_opindex, event_com)
	}, component1)
	return 
}
