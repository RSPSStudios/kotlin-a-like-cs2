// 1632 clientscript
fun script1632(keychar0: Char, comsubid1: Int, component2: Component, string0: String, string1: String, clock3: Int) {

	if (clientclock < clock3) {
		return 
	}
	if (~keypress_permit(458, -1) = 0) {
		return 
	}
	if (keychar0 ! null & (string_indexof_char(string0, keychar0) ! -1 | string_indexof_char(string1, keychar0) ! -1)) {
		if_setonkey({
			script1632(event_keychar, comsubid1, event_com, string0, string1, calc(clientclock + 20))
		}, component2)
		if (cc_find(component2, 4) = ^true) {
			cc_settrans(120)
			cc_setontimer({
				cc_settrans_temporarily_end(event_com, event_comsubid, 0, calc(clientclock + 10))
			})
		}
		sound_synth(synth_2266, 1, 0)
		if (cc_find(interface_458:2, comsubid1) = ^true) {
			cc_resume_pausebutton
		}
	}
	return 
}
