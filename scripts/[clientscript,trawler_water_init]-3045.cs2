// 3045 clientscript
fun trawler_water_init(component0: Component) {

	cc_deleteall(component0)
	cc_create(component0, ^iftype_rectangle, 0)
	.cc_create(component0, ^iftype_rectangle, 1)
	cc_setposition(0, 0, ^setpos_abs_left, ^setpos_abs_centre)
	.cc_setposition(0, 0, ^setpos_abs_right, ^setpos_abs_centre)
	cc_settrans(128)
	.cc_settrans(128)
	cc_setfill(true)
	.cc_setfill(true)
	cc_setcolour(0x7f7fff)
	.cc_setcolour(^black)
	~trawler_water_update(component0)
	if_setonvartransmit(component0)  {
		trawler_water_update(event_com, cc_getid, .cc_getid){VARP[391]}
	}
	return 
}
