		worldmap_overview_click(event_mousex, event_mousey, -1, event_com, int11, int12, int9, int10, worldmap_getconfigbounds(maparea0))
	}
	~worldmap_applyzoom(worldmap_getconfigzoom(maparea0), component7, component8)
	return 
}
