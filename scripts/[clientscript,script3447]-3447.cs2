// 3447 clientscript
fun script3447(opindex0: Int, component1: Component, comsubid2: Int, trans3: Int) {

	if (opindex0 = 1 & cc_find(component1, comsubid2) = ^true) {
		cc_settrans(140)
		cc_setonmouserepeat(null)
		cc_setonmouseleave(null)
		cc_setonmouseleave(null)
		cc_setontimer({
			script3448(calc(clientclock + 20), component1, comsubid2, trans3)
		})
	}
	return 
}
