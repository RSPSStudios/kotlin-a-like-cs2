// 756 clientscript
fun cc_flasher(component0: Component, comsubid1: Int, clock2: Int) {

	val int3: Int = 0
	val int4: Int = 0
	val int5: Int = 0
	if (cc_find(component0, comsubid1) = ^true) {
		int4 = clientclock
		int5 = calc(int4 - clock2)
		if (int5 >= 750) {
			cc_settrans(255)
			cc_setontimer(null)
			return 
		}
		if (calc(int4 % 40) >= 20) {
			cc_settrans(255)
			return 
		}
		cc_settrans(interpolate(0, int5, 0, 750, 255))
	}
	return 
}
