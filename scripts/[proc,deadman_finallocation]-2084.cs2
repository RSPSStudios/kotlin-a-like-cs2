// 2084 proc
fun deadman_finallocation(component0: Component) {

	val text0: String = "North of Falador"
	if (VARBIT[1497] = 2) {
		text0 = "Demonic Ruins (Annakarl)"
	}
	if (~wildwars_world = 1) {
		if_settext("The Graveyard of Shadows", component0)
	} else {
		if_settext(text0, component0)
	}
	return 
}
