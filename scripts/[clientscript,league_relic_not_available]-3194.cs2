// 3194 clientscript
fun league_relic_not_available(int0: Int) {

	sound_synth(pillory_wrong, 1, 0)
	if (int0 = 1) {
		mes("You have already unlocked a Relic from this tier.")
	} else if (int0 = 3) {
		mes("You should unlock a Relic from the previous tier first.")
	} else if (int0 = 0) {
		mes("You need to earn more League Points to unlock a Relic from this tier.")
	}
	return 
}
