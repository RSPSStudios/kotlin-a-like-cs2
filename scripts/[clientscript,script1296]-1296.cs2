// 1296 clientscript
fun script1296(clock0: Int, component1: Component, comsubid2: Int, component3: Component, comsubid4: Int, comsubid5: Int) {

	if (clientclock < clock0) {
		return 
	}
	if (cc_find(component1, comsubid2) = ^true) {
		cc_setontimer(null)
	}
	if (cc_find(component3, comsubid4) = ^true) {
		cc_sethide(true)
	}
	if (cc_find(component3, comsubid5) = ^true) {
		cc_sethide(true)
	}
	return 
}
