// 3733 clientscript
fun slideshow_timer(component0: Component, enum1: Enum, int2: Int, int3: Int, clock4: Int) {

	val int5: Int = calc(int2 * 50)
	val int6: Int = calc(clientclock - clock4)
	if (int6 = 0) {
		return 
	}
	if (calc(int6 % int5) = 0) {
		~slideshow_transition(component0, enum1, int2, int3)
	}
	return 
}
