// 508 proc
fun bankmain_tabicon_highlight(int0: Int) {

	if (VARBIT[4150] = int0) {
		.cc_setgraphic("banktabs,2")
		return 
	}
	.cc_setgraphic("banktabs,0")
	val graphic1: Graphic = "banktabs,1"
	cc_setonmouserepeat({
		cc_graphic_swapper(event_com, .cc_getid, graphic1, null)
	})
	graphic1 = "banktabs,0"
	cc_setonmouseleave({
		cc_graphic_swapper(event_com, .cc_getid, graphic1, null)
	})
	return 
}
