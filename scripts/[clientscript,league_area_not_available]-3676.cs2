fun league_area_not_available(int0: Int) {

	sound_synth(pillory_wrong, 1, 0)
	if (int0 = 1) {
		mes("You have already unlocked this area.")
	} else if (int0 = 0) {
		mes("You need to complete more tasks to unlock this area.")
	} else if (int0 = 3) {
		mes("You must unlock Karamja as your second area.")
	} else if (int0 = 4) {
		mes("You cannot unlock any more areas.")
	}
	return 
}
