// 1992 proc
fun gravestone_retrieval_items(component0: Component, component1: Component) {

	val obj2: Obj = null
	val height3: Int = 0
	val int4: Int = 0
	while (int4 < 120) {
		if (cc_find(component0, int4) = ^true) {
			obj2 = inv_getobj(inv_525, int4)
			if (obj2 ! null) {
				cc_sethide(false)
				cc_setobject(obj2, inv_getnum(inv_525, int4))
				cc_setopbase("<col=ff9040>${oc_name(obj2)}</col>")
				cc_settrans(0)
				cc_setonop({
					cc_settrans_temporarily(event_com, event_comsubid, 100, 0)
				})
				cc_setontimer(null)
				height3 = calc(cc_gety + cc_getheight)
			} else {
				cc_sethide(true)
			}
		}
		int4 = calc(int4 + 1)
	}
	if (height3 > if_getheight(component0)) {
		if_setscrollpos(0, ~min(if_getscrolly(component0), calc(height3 - if_getheight(component0))), component0)
		if_setscrollsize(0, height3, component0)
	} else {
		if_setscrollpos(0, 0, component0)
		if_setscrollsize(0, 0, component0)
	}
	~scrollbar_vertical_rebuild(component1, component0)
	return 
}
