// 3735 clientscript
fun slideshow_fadeout(component0: Component, comsubid1: Int, int2: Int) {

	if (cc_find(component0, comsubid1) = ^true) {
		if (cc_gettrans = 255) {
			cc_setontimer(null)
			cc_setgraphic(null)
		} else {
			cc_settrans(~minmax(0, 255, calc(cc_gettrans + int2)))
		}
	}
	return 
}
